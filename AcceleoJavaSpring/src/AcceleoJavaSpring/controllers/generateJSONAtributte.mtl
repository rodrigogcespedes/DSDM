[comment encoding = UTF-8 /]
[module generateJSONAtributte('http://www.example.org/diagramaClasesCompleto')]


[template public generateJSONAtributte(clase : Clase)]
[for (a : Atributo | clase.atributos_contenidos) separator(',\n')]

[if (a.tipo = TipoDato::String or a.tipo = TipoDato::char)]
	"[a.nombre/]": "[a.nombre/]"
[elseif(a.tipo = TipoDato::Date)]
	"[a.nombre/]": "2020-09-10"
[else]
	"[a.nombre/]": 1
[/if]

[/for]
[/template]
